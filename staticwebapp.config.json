{
    "routes": [
        {
            "route": "/api/Generate",
            "allowedRoles": [
                "admin",
                "customer"
            ]
        },
        {
            "route": "/api/LogLine/*",
            "allowedRoles": [
                "authenticated"
            ]
        },
        {
            "route": "/api/SGAdmin/*",
            "allowedRoles": [
                "authenticated",
                "admin"
            ]
        },
        {
            "route": "/.auth/login/github",
            "statusCode": 404
        },
        {
            "route": "/.auth/login/twitter",
            "statusCode": 404
        }
    ],
    "navigationFallback": {
        "rewrite": "/index.html"
    }
}